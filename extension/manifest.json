{
  "name": "Incognito Link",
  "description": "Open a link in Incognito mode with ⇧ + Alt or ⇧ + ⌥",
  "homepage_url": "https://github.com/francoischalifour/incognito-link",
  "manifest_version": 2,
  "version": "1.0.1",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "app.js"
      ],
      "run_at": "document_start"
    }
  ]
}
